{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:49937", //Сторінка яка відкриватиметься в браузері в першу чергу при дебазі в режимі iisExpress
      "sslPort": 44358 //Порт локального сервера
    }
  },
  "profiles": {
    "IIS Express": //При запуску в режимі IIS Express 
    {
      "commandName": "IISExpress",
      "launchBrowser": true, //буде запускатись браузер
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "ASP_NET_CORE_SHOP": //При запуску в режимі IIS Express 
    {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "applicationUrl": "https://localhost:5001;http://localhost:5000", //відкриється сторінка в браузері по силці .https://localhost:5001 аюл .https://localhost:5000
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
